{
    "name": "nearata/flarum-ext-signup-confirm-password",
    "description": "Adds a confirm password field in the Sign Up modal.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.14"
    },
    "authors": [
        {
            "name": "William Di Cicco",
            "email": "williamdicicco@protonmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nearata\\SignUpConfirmPassword\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Sign Up Confirm Password",
            "icon": {
                "name": "fas fa-check-double",
                "backgroundColor": "#2896bf",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": ""
        }
    }
}
